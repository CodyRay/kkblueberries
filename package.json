{
  "name": "k-and-k-blueberries",
  "homepage": "http://www.kandkblueberries.com/",
  "version": "0.1.0",
  "private": true,
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "critical": "^0.9.1",
    "eslint": "^3.19.0",
    "express": "^4.17.1",
    "gh-pages": "^1.2.0",
    "glob-promise": "^3.4.0",
    "gulp-promise": "^1.0.2",
    "js-yaml": "^3.14.0",
    "jsdom": "^9.12.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.5",
    "react-dom": "^15.6.2",
    "react-scripts": "1.1.5",
    "sharp": "^0.25.3",
    "smartcrop-sharp": "^2.0.3"
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "bootstrap": "^3.4.1",
    "npm-check-updates": "^7.0.0",
    "prettier": "^2.0.5",
    "react": "^15.6.2",
    "react-code-splitting": "^1.2.1",
    "react-helmet": "^5.2.1",
    "react-modal": "^1.9.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "scripts": {
    "start": "npm run preprocess && react-scripts start",
    "build": "npm run preprocess && react-scripts build && babel-node react-snapshot/react-snapshot.js",
    "test": "react-scripts test --env=jsdom",
    "eslint": "eslint src/",
    "eslint:fix": "eslint src/ --fix",
    "eject": "react-scripts eject",
    "preprocess": "node gallery.js ./src/photos",
    "predeploy": "npm run build && babel-node post_optimization.js",
    "deploy": "gh-pages -a -d build"
  }
}
